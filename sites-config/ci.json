{
  "artifactStructure": {
    "assets": [
      {
        "root": "apps/test-site/dist",
        "pattern": "apps/test-site/assets/**/*"
      }
    ],
    "features": "apps/test-site/sites-config/features.json"
  },
  "dependencies": {
    "installDepsCmd": "npm install",
    "requiredFiles": ["package.json", "package-lock.json", ".npmrc"]
  },
  "buildArtifacts": {
    "buildCmd": "npm run build:local"
  },
  "livePreview": {
    "serveSetupCmd": "npm run build && cd apps/test-site && npm run localData",
    "serveCmd": "cd apps/test-site && npm run dev"
  }
}